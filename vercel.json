{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "github": {
    "enabled": false
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
